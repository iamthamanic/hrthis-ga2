{
  "name": "hrthis",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-sonarjs": "^3.0.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.523.0",
    "openai": "^4.89.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.1.1",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^3.2.0",
    "typescript": "^4.9.5",
    "uuid": "^11.1.0",
    "zod": "^3.25.67",
    "zustand": "^5.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --testResultsProcessor=jest-sonar-reporter",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,json,css,md}",
    "quality:check": "node scripts/quality-check.js",
    "quality:summary": "node scripts/quality-summary.js",
    "docs:generate": "node scripts/generate-docs.js",
    "readme:update": "node scripts/update-readme.js",
    "pre-commit": "lint-staged",
    "prepare": "husky install || true"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.17"
  }
}
