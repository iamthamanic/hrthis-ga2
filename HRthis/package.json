{
  "name": "hrthis",
  "version": "2.3.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-sonarjs": "^3.0.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.523.0",
    "openai": "^4.89.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.7.0",
    "react-scripts": "5.0.1",
    "serve": "^14.2.4",
    "tailwind-merge": "^3.2.0",
    "typescript": "^4.9.5",
    "uuid": "^11.1.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npm run performance:check",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --testResultsProcessor=jest-sonar-reporter",
    "test:watch": "react-scripts test --watchAll",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:strict": "eslint src --ext .ts,.tsx --config .eslintrc.strict.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write src/**/*.{ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,json,css,md}",
    "quality:check": "npm run lint && npm run type-check && npm run test:coverage",
    "quality:summary": "node scripts/quality-summary.js",
    "performance:check": "node scripts/check-performance.js",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "lighthouse": "lhci autorun",
    "security:check": "npm audit --audit-level=high",
    "docs:generate": "node scripts/generate-docs.js",
    "readme:update": "node scripts/update-readme.js",
    "pre-commit": "lint-staged",
    "prepare": "husky install || true",
    "validate": "npm run lint && npm run type-check && npm run test:coverage && npm run performance:check",
    "ci": "npm run validate && npm run build && npm run test:e2e"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "eslint-plugin-import": "^2.32.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.17"
  }
}
